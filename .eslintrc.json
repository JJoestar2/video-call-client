{
  "extends": [
    "plugin:prettier/recommended",
    "next/core-web-vitals"
  ],
  "rules": {
    "no-unused-vars": "off",
    "import/order": [
      "error",
      {
        "groups": [
          [
            "builtin",
            "external"
          ],
          "internal",
          [
            "parent",
            "sibling"
          ],
          "index",
          "object"
        ]
      }
    ]
  }
}
